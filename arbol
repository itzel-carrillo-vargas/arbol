      printf("3=tios \n"); 	
      printf("4= papas\n");
      printf("5=hermanos\n");
      printf("6=sobrinos \n");
      printf("7= \n");
      
	  int op;
	  
      scanf("%d",& op);
	   	
	  switch(op){
	  	
	case 1 :
	  	printf("\n no se");
	  	break;
	case 2:
	  	printf("\n dolores azpeitia");
	  	break;
	case 3:
	  	printf("\n marcela carrillo ");
	  	break;
